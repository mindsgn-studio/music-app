{
  "name": "MIXO",
  "version": "2.7.0",
  "private": true,
  "scripts": {
    "postinstall": "rn-nodeify --install fs,dgram,process,path,console,stream,crypto --hack",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android:beta": "yarn version --minor && fastlane android beta",
    "android:release": "yarn version --major && fastlane android release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && git add -A ."
    }
  },
  "dependencies": {
    "@klarna/react-native-vector-drawable": "^0.4.0",
