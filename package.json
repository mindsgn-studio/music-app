    "@realm/react": "^0.4.3",
    "@walletconnect/react-native-dapp": "^1.8.0",
    "buffer": "^6.0.3",
    "console-browserify": "^1.2.0",
    "expo-gl": "^12.4.0",
    "expo-three": "^7.0.0",
    "gsap": "^3.11.5",
    "jsmediatags": "^3.9.7",
    "path-browserify": "^0.0.0",
    "process": "^0.11.10",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-crypto": "^2.2.0",
    "react-native-fontawesome": "^7.0.0",
